{
  "name": "refreshResumeData",
  "handler": "src/index.handler",
  "runtime": "nodejs18.x",
  "memory": 256,
  "timeout": 60,
  "api": {
    "path": "/refreshResumeData",
    "authorization": "COGNITO_USER_POOLS",
    "methods": ["POST", "OPTIONS"]
  },
  "permissions": {
    "lambda": {
      "enhanceApplicationsWithResumeData": [
        "execute"
      ]
    }
  },
  "environment": {
    "ENHANCE_LAMBDA_NAME": "enhanceApplicationsWithResumeData-${AMPLIFY_ENV}"
  }
} 